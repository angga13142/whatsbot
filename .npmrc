# Save exact versions (not ^ or ~)
save-exact=true

# Always save to dependencies (not devDependencies by default)
save=true

# Package lock
package-lock=true

# Audit level
audit-level=moderate

# Progress bar
progress=true

# Optional: Use specific registry
# registry=https://registry.npmjs.org/

# Optional: Set npm log level
# loglevel=warn
